    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-PA5Main<garden>",
            "request": "launch",
            "mainClass": "garden.PA5Main",
            "projectName": "garden",
            "args": "${command:SpecifyProgramArgs}"
        }
    ]
}